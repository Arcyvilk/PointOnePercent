 SidebarSection(props: SidebarSectionProps) {\n  const { section } = props;\n  const getSection = (section: TSection) => {\n    switch (section) {\n      case 'update':\n        return <SectionUpdate />;\n      case 'trivia':\n        return <SectionTrivia />;\n      case 'top':\n        return <SectionTop />;\n      case 'history':\n        return <SectionHistory />;\n      case 'sales':\n        return <SectionSale />;\n      default:\n        return null;\n    }\n  };\n\n  return getSection(section);\n}\n","X:\\Documents\\Self-study\\PointOnePercent\\src\\components\\sidebar\\index.ts",[],"X:\\Documents\\Self-study\\PointOnePercent\\src\\components\\sidebar\\Sections\\SectionTop\\index.tsx",["255","256"],"import React from 'react';\nimport styled from 'styled-components';\nimport { orderBy } from 'lodash';\nimport { useSelector } from 'react-redux';\nimport { SmallMember, Section, SectionTitle } from '../../';\n\nconst FlexColumn = styled.div`\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n`;\n\nexport default function SectionTop(): JSX.Element {\n  const members = useSelector((state: any) =>\n    orderBy(\n      state.members,\n      [member => (member.points ? member.points : 0)],\n      ['desc'],\n    ).slice(0, 10),\n  );\n\n  const memberRow = (member: any, index: number) => (\n    <SmallMember key={`sidebar-member-${index}`}>\n      <div>{index + 1}.</div>\n      <div>\n        <span className=\"bold\">{member.name}</span>\n      </div>\n      <div>{member.points} pts</div>\n    </SmallMember>\n  );\n\n  return (\n    <Section>\n      <SectionTitle>Top 10 members</SectionTitle>\n      <FlexColumn>\n        {members.map((member, memberIndex) => memberRow(member, memberIndex))}\n      </FlexColumn>\n    </Section>\n  );\n}\n","X:\\Documents\\Self-study\\PointOnePercent\\src\\components\\sidebar\\Sections\\SectionUpdate\\index.tsx",["257","258","259"],"import React, { useState, useEffect } from 'react';\nimport axios from 'axios';\nimport { Section, SectionTitle } from '../../';\n\nexport default function SectionUpdate(): JSX.Element {\n  const updateTimeout = 43200000;\n  const [lastUpdated, setLastUpdated] = useState(\n    undefined as number | undefined,\n  );\n  const updating = false; // TODO connect to websocket\n  const updateStatus = 50; // TODO connect to websocket\n\n  const sendUpdateRequest = (): void => {\n    const url = '/rest/update';\n    axios\n      .get(url)\n      .then(res => console.log(res.data.content))\n      .catch(err => console.log(err));\n  };\n\n  const getUpdateDate = (): void => {\n    const url = '/rest/status';\n    axios\n      .get(url)\n      .then(res => setLastUpdated(res.data.lastUpdated))\n      .catch(err => {\n        setLastUpdated(undefined);\n        console.log(err);\n      });\n  };\n\n  const timeoutBeforeUpdate = (): number => {\n    const timeout = Math.ceil(\n      (updateTimeout - (Date.now() - Number(lastUpdated))) / 60000,\n    );\n    return timeout;\n  };\n\n  const blockUpdateIfTooSoon = (): boolean => timeoutBeforeUpdate() > 0;\n\n  useEffect(() => {\n    getUpdateDate();\n  }, []);\n\n  const nextUpdate = lastUpdated\n    ? new Date(lastUpdated + 43200000).toLocaleString()\n    : 'unknown';\n\n  const timeToUpdate = `${Math.ceil(\n    (updateTimeout - (Date.now() - Number(lastUpdated))) / 3600000,\n  )}:${Math.ceil(\n    ((updateTimeout - (Date.now() - Number(lastUpdated))) % 3600000) / 60000,\n  )} hours till you can update again`;\n\n  return (\n    <Section>\n      <SectionTitle\n        style={{ height: '100%' }}>{`Next update: ${nextUpdate}`}</SectionTitle>\n      <h3 className=\"section-title\">\n        Last updated: {new Date(Number(lastUpdated)).toLocaleString()}\n      </h3>\n      <div className=\"flex-column\">\n        {updating ? (\n          <div\n            className=\"update-progress-bar-border\"\n            title={`${updateStatus}%`}>\n            <div\n              className=\"update-progress-bar\"\n              style={{ width: `${updateStatus}%` }}></div>\n            <div className=\"update-progress-bar-percentage\">Updating...</div>\n          </div>\n        ) : (\n          <button\n            className={\n              blockUpdateIfTooSoon()\n                ? 'custom-button update-button button-blocked'\n                : 'custom-button update-button'\n            }\n            onClick={() =>\n              blockUpdateIfTooSoon() ? null : sendUpdateRequest()\n            }\n            title={blockUpdateIfTooSoon() ? timeToUpdate : 'Update'}>\n            Update\n          </button>\n        )}\n      </div>\n    </Section>\n  );\n}\n","X:\\Documents\\Self-study\\PointOnePercent\\src\\components\\sidebar\\Sections\\SectionHistory\\index.tsx",["260","261","262","263","264","265","266"],"import React from 'react';\nimport { orderBy } from 'lodash';\nimport { useSelector } from 'react-redux';\nimport { swapRatingToIcon } from '../../../../shared/helpers/helper';\nimport { SmallEvent, Section, SectionTitle } from '../../';\n\nexport default function SectionHistory(): JSX.Element {\n  const events = useSelector((state: any) =>\n    orderBy(state.events, ['date'], ['desc']).slice(0, 10),\n  );\n  const games = useSelector((state: any) => state.games);\n  const members = useSelector((state: any) => state.members);\n  const rating = useSelector((state: any) => state.rating);\n\n  const sortEvents = (event: any, eventIndex: number) => {\n    const player = members.find(\n      (m: any) => Number(m.id) === Number(event.member),\n    );\n    const game = games.find((g: any) => Number(g.id) === Number(event.game));\n\n    switch (event.type) {\n      case 'memberJoined':\n        return player ? (\n          <SmallEvent key={`sidebar-event-${eventIndex}`}>\n            <i className=\"fas fa-user-plus\"></i>\n            <span className=\"bold\"> {player.name}</span> has joined the group!\n          </SmallEvent>\n        ) : null;\n      case 'memberLeft':\n        return player ? (\n          <SmallEvent key={`sidebar-event-${eventIndex}`}>\n            <i className=\"fas fa-user-minus\"></i>\n            <span className=\"bold\"> {player.name}</span> has left the group!\n          </SmallEvent>\n        ) : null;\n      case 'newGame':\n        return game ? (\n          <SmallEvent key={`sidebar-event-${eventIndex}`}>\n            <i className=\"fas fa-plus-square\"></i>\n            <span className=\"bold\"> {game.title}</span> has been curated!\n          </SmallEvent>\n        ) : null;\n      case 'complete':\n        return player && game ? (\n          <SmallEvent key={`sidebar-event-${eventIndex}`}>\n            <i className=\"fas fa-check-square\"></i>\n            <span className=\"bold\"> {player.name}</span> completed{' '}\n            <span className=\"bold\">{game.title}</span>!\n          </SmallEvent>\n        ) : null;\n      case 'tierChange':\n        return game ? (\n          <SmallEvent key={`sidebar-event-${eventIndex}`}>\n            <i className=\"fas fa-undo-alt\"></i>\n            <span className=\"bold\"> {game.title}</span> changed its tier to{' '}\n            <i className={swapRatingToIcon(game.rating, rating)} />!\n          </SmallEvent>\n        ) : null;\n      case 'achievementNumberChange':\n        return game ? (\n          <SmallEvent key={`sidebar-event-${eventIndex}`}>\n            <i className=\"fas fa-tasks\"></i>\n            <span className=\"bold\"> {game.title}</span>{' '}\n            {event.oldNumber < event.newNumber\n              ? `got ${event.newNumber - event.oldNumber} new achievements!`\n              : `had ${\n                  event.oldNumber - event.newNumber\n                } achievements removed!`}\n          </SmallEvent>\n        ) : null;\n      case 'custom': {\n        const { content } = event;\n        if (!content) {\n          return null;\n        }\n        const { text, icon } = content;\n        if (!text) {\n          return null;\n        }\n\n        return (\n          <SmallEvent key={`sidebar-event-${eventIndex}`}>\n            <i className={icon ? icon : 'fas fa-birthday-cake'}></i>{' '}\n            {text &&\n              text.split('#').map((str: string, index: number) => {\n                if (index % 2 === 1) {\n                  return <span className=\"bold\">{str}</span>;\n                }\n                return str;\n              })}\n          </SmallEvent>\n        );\n      }\n      default:\n        return null;\n    }\n  };\n\n  return (\n    <Section>\n      <SectionTitle>Last events</SectionTitle>\n      {events.map((event, eventIndex) => sortEvents(event, eventIndex))}\n    </Section>\n  );\n}\n","X:\\Documents\\Self-study\\PointOnePercent\\src\\components\\sidebar\\Sections\\SectionTrivia\\index.tsx",["267","268","269","270","271"],"import React, { useState, useEffect } from 'react';\nimport { useSelector } from 'react-redux';\nimport axios from 'axios';\nimport { Section, SectionTitle } from '../../';\n\nexport default function SectionTrivia(): JSX.Element {\n  const members = useSelector((state: any) =>\n    state.members.filter((member: any) => member.member),\n  );\n  const rating = useSelector((state: any) => state.rating);\n  const [games, setGames] = useState({\n    total: 0,\n    all: [],\n  });\n\n  const loadGames = async (): Promise<void> => {\n    const response = await axios.get('/rest/api/games');\n    if (!response?.data) {\n      return;\n    }\n    setGames({\n      total: response?.data?.length,\n      all: response?.data,\n    });\n  };\n\n  const mapCurated = () => {\n    if (rating) {\n      return rating.map((tier: any, index: number) => (\n        <li style={{ marginLeft: '30px' }} key={`${tier.score}-${index}`}>\n          <i className={tier.icon} />\n          <span className=\"bold\">{` : ${\n            games.all.filter((game: any) => game.rating === tier.id).length\n          }`}</span>\n        </li>\n      ));\n    }\n  };\n\n  useEffect(